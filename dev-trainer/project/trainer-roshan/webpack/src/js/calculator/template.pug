.calculator__display 
    .calculator__display-output 0
.calculator__body 
    button.calculator__body-key(data-operator='add') +
    button.calculator__body-key(data-operator='subtract') -
    button.calculator__body-key(data-operator='multiply') &times
    button.calculator__body-key(data-operator='divide') /
    button.calculator__body-key(data-value='7') 7
    button.calculator__body-key(data-value='8') 8
    button.calculator__body-key(data-value='9') 9
    button.calculator__body-key(data-action='clear') CE
    button.calculator__body-key(data-value='4') 4
    button.calculator__body-key(data-value='5') 5
    button.calculator__body-key(data-value='6') 6
    button.calculator__body-key.calculator__body-equal(data-action='equals') =
    button.calculator__body-key(data-value='1') 1
    button.calculator__body-key(data-value='2') 2
    button.calculator__body-key(data-value='3') 3
    button.calculator__body-key.calculator__body-zero(data-value='0') 0
    button.calculator__body-key(data-prefix='point') .
